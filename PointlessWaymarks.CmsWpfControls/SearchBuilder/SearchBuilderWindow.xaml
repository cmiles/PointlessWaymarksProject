<Window x:Class="PointlessWaymarks.CmsWpfControls.SearchBuilder.SearchBuilderWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PointlessWaymarks.CmsWpfControls.SearchBuilder"
        mc:Ignorable="d"
        Title="Search Builder" Height="450" Width="800" d:DataContext="{d:DesignInstance local:SearchBuilderWindow}">
    <Grid>
        <local:SearchBuilderControl DataContext="{Binding SearchContext}"></local:SearchBuilderControl>
    </Grid>
</Window>
