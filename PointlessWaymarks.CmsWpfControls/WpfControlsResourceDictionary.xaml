                            <b:EventTrigger EventName="SelectionChanged">
                                <wpfBehavior:EventToCommand Command="{Binding SelectedTextTracker.SelectedTextChangedCommand}" PassEventArgsToCommand="True" />
                            </b:EventTrigger>
                        </b:Interaction.Triggers>
                    </TextBox>
                    <WrapPanel Margin="0,6,0,0" Orientation="Horizontal">
                        <TextBlock VerticalAlignment="Center" TextWrapping="Wrap">
                            <Hyperlink Command="{Binding ItemActions.ViewOnSiteCommand}" CommandParameter="{Binding DbEntry}">
                                <Hyperlink.Inlines>
                                    <Run Text="{Binding DbEntry.Url}" />
                                </Hyperlink.Inlines>
                            </Hyperlink>
                        </TextBlock>
                        <Button
                            Width="80"
                            Height="20"
                            VerticalAlignment="Center"
                            Command="{Binding ItemActions.CopyUrlCommand}"
                            CommandParameter="{Binding DbEntry.Url}">
                            Copy Link
                        </Button>
                    </WrapPanel>
                </StackPanel>
                <ContentPresenter
                    Grid.Row="1"
                    Margin="2"
                    VerticalAlignment="Bottom"
                    Content="{StaticResource ContentListContentIdAndDateDisplay}" />
            </Grid>
        </Border>
