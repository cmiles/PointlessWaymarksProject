<Window
    x:Class="PointlessWaymarks.CmsWpfControls.PhotoList.PhotoListWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:photoList="clr-namespace:PointlessWaymarks.CmsWpfControls.PhotoList"
    Title="{Binding WindowTitle}"
    Width="900"
    Height="800"
    d:DataContext="{d:DesignInstance photoList:PhotoListWindow}"
    mc:Ignorable="d">
    <Grid>
        <photoList:PhotoListWithActionsControl x:Name="PhotoListControl" DataContext="{Binding PhotoListContext}" />
    </Grid>
</Window>