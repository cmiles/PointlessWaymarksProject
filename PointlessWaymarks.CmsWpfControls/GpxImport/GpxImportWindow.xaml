<Window x:Class="PointlessWaymarks.CmsWpfControls.GpxImport.GpxImportWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PointlessWaymarks.CmsWpfControls.GpxImport"
        mc:Ignorable="d"
        Title="GpxImportWindow" Height="450" Width="800" d:DataContext="{d:DesignInstance local:GpxImportWindow}">
    <Grid Margin="4">
        <local:GpxImportControl DataContext="{Binding ImportContext}"></local:GpxImportControl>
    </Grid>
</Window>
